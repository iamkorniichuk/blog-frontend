{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "version": 2,
    "name": "blog",
    "rewrites": [
        {
            "source": "/:path*",
            "destination": "/api"
        }
    ],
    "functions": {
        "api/index.mjs": {
            "includeFiles": "dist/blog/**"
        }
    }
}